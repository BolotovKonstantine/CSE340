<h1><%- title %></h1>
<%- messages() %>
<form class="account" id="loginForm" action="/account/login" method="post">
    <label>Email<input type="email" name="account_email" required placeholder="Enter a valid email address"></label>
    <label>Password<input type="password" name="account_password" id="password" required></label>
    <input type="submit" value="LOGIN">
</form>
<div>
    <span>No account? <a href="/account/registration">Sign up</a></span>
</div>